		"esbuild": "0.25.5",
		"eslint": "^9.30.1",
		"obsidian": "latest",
		"tslib": "2.8.1",
		"typescript": "5.5.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/toole2525/mermaid-view-enhancer"
	},
	"bugs": {
		"url": "https://github.com/toole2525/mermaid-view-enhancer/issues"
